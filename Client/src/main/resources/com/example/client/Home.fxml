<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="419.0" prefWidth="476.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.ApplicationController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox alignment="CENTER" prefHeight="366.0" prefWidth="436.0">
         <children>
            <VBox alignment="CENTER" prefHeight="366.0" prefWidth="164.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome on our online chat !" />
                  <TextField fx:id="nicknameField" promptText="Please choose a nickname..">
                     <VBox.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </VBox.margin>
                  </TextField>
                  <Button mnemonicParsing="false" onMouseClicked="#login" text="Login" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
